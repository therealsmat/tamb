<!--Start header-->
<header class="themeix-header">
  {{> 'header_top'}}
   <div class="header-bottom">
      <div class="container">
         <div class="row">
            <div class="col-md-12">
            <a class="navbar-brand float-left" href="{{@blog.url}}"><img src="{{#if @blog.logo}}{{@blog.logo}}{{else}}/assets/images/header-icon.png{{/if}}" alt="{{@blog.title}}"/></a>
               {{navigation}}
            </div>
         </div>
      </div>
   </div>
  <!-- Modal -->
  <div class="modal fade" id="themeix_search"  role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Search Here</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body p-4">
           <form>
              <input type="search" class="form-control" id="search-field" placeholder="search here" autocomplete="off" />
           </form>
           <div class="search-result"></div>
        </div>
      </div>
    </div>
  </div>
</header>
<!--End header-->

